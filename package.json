{
    "name": "vite-vue3-starter",
    "version": "0.0.1",
    "private": false,
    "description": "A Vite2.x + Vue3 + TypeScript template starter",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "start:local": "vite --mode=self",
        "build": "vite build",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/XPoet/vite-vue3-starter.git"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript",
        "Starter"
    ],
    "author": "XPoet",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/XPoet/vite-vue3-starter/issues"
    },
    "homepage": "https://github.com/XPoet/vite-vue3-starter#readme",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "@popjs/util": "git@github.com:imvkmark/popjs-util.git",
        "@vant/touch-emulator": "^1.3.2",
        "axios": "^0.21.1",
        "better-mock": "^0.3.2",
        "crypto-js": "^4.0.0",
        "lodash-es": "^4.17.21",
        "vant": "^3.1.4",
        "vue": "^3.0.5",
        "vue-router": "^4.0.6",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.1",
        "@commitlint/config-conventional": "^12.1.1",
        "@types/crypto-js": "^4.0.2",
        "@types/jest": "^26.0.22",
        "@types/lodash-es": "^4.17.4",
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "@vitejs/plugin-vue": "^1.2.1",
        "@vue/compiler-sfc": "^3.0.5",
        "@vue/test-utils": "^2.0.0-rc.4",
        "eslint": "^7.24.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.5",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-vue": "^7.8.0",
        "jest": "^26.6.3",
        "less": "^4.1.1",
        "lint-staged": "^10.5.4",
        "ts-jest": "^26.5.4",
        "typescript": "^4.1.3",
        "vite": "^2.1.5",
        "vite-plugin-style-import": "^0.9.2",
        "vue-jest": "^5.0.0-alpha.7",
        "vue-tsc": "^0.0.15"
    }
}
