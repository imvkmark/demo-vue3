{
    "name": "vant-demo",
    "version": "0.0.1",
    "private": false,
    "description": "A Vite2.x + Vue3 + TypeScript template starter",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "start:local": "vite --mode=self",
        "build": "vite build",
        "build:local": "vite build --mode=self",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:imvkmark/vant-demo.git"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript"
    ],
    "author": "XPoet",
    "license": "MIT",
    "homepage": "https://github.com/imvkmark/vant-demo",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "@popjs/util": "^0.3",
        "@vant/touch-emulator": "^1.3.2",
        "axios": "^1.2.2",
        "better-mock": "^0.3.2",
        "crypto-js": "^4.0.0",
        "easytimer.js": "^4.5.4",
        "lodash-es": "^4.17.21",
        "vant": "^3.6.11",
        "vue": "^3.0.5",
        "vue-router": "^4.0.6",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/lodash-es": "^4.17.4",
        "@types/node": "^16.4.13",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.47.1",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/compiler-sfc": "^3.0.5",
        "consola": "^2.15.3",
        "eslint": "^8.30.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-vue": "^9.8.0",
        "less": "^4.1.1",
        "lint-staged": "^13.1.0",
        "typescript": "^4.9.4",
        "unplugin-vue-components": "^0.22.12",
        "vite": "^4.0.3",
        "vite-plugin-mkcert": "^1.10.1",
        "vite-plugin-style-import": "^2.0.0",
        "vue-tsc": "^1.0.18"
    }
}
