{
    "name": "vant-demo",
    "version": "0.0.1",
    "private": false,
    "description": "A Vite2.x + Vue3 + TypeScript template starter",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "start:local": "vite --mode=self",
        "build": "vite build",
        "build:local": "vite build --mode=self",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:imvkmark/vant-demo.git"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript"
    ],
    "author": "XPoet",
    "license": "MIT",
    "homepage": "https://github.com/imvkmark/vant-demo",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "@popjs/util": "^0.3",
        "@vant/touch-emulator": "^1.3.2",
        "axios": "^1.4.0",
        "better-mock": "^0.3.2",
        "crypto-js": "^4.0.0",
        "easytimer.js": "^4.5.4",
        "lodash-es": "^4.17.21",
        "pinia": "^2.1.6",
        "ua-parser-js": "^1.0.35",
        "vant": "^4.6.4",
        "vue": "^3.3.4",
        "vue-router": "^4.2.4"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/lodash-es": "^4.17.8",
        "@types/node": "~18",
        "@types/ua-parser-js": "^0.7.36",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/compiler-sfc": "^3.3.4",
        "consola": "^2.15.3",
        "eslint": "^8.46.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-vue": "^9.17.0",
        "less": "^4.2.0",
        "lint-staged": "^13.2.3",
        "typescript": "^4.9.4",
        "unplugin-vue-components": "^0.25.1",
        "vite": "^4.4.9",
        "vue-tsc": "^1.8.8"
    }
}
